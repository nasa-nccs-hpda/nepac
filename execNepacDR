while getopts f:m:e:n: flag
do
	case "${flag}" in
		f) inputcsv=${OPTARG};;
		m) inputfile=${OPTARG};;
		e) erroreddata=${OPTARG};;
		n) nodata=${OPTARG};;
	esac
done
module load singularity
singularity run -B /att /att/nobackup/cssprad1/nepac-sandbox.sif \
/usr/bin/python \
--celery \
/usr/local/nepac/view/NepacCommandLineView.py \
-d /usr/local/nepac/model/datasets \
-f $inputcsv \
-md_file $inputfile \
-errored_data $erroreddata \
-no_data $nodata
